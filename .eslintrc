
{
  "extends": ["airbnb/base"],
  "rules": {
    "global-require": 0,
    "id-length": 0,
    "max-len": 0,
    "import/default": 0,
    "no-param-reassign": [2, { "props": false }],
    "consistent-return": 0,
    "arrow-parens": 0,
    "import/no-dynamic-require": 0,
    "newline-per-chained-call": 0,
    "object-curly-newline": ["error", {
      "ObjectPattern": { "multiline": false }
    }]
  }
}